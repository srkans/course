@model List<double>
    <table>
        <thead>
            <tr>
                <th>Kur Deger</th>
                <th>Tarih</th>
            </tr>
        </thead>
        <tbody>
            @{ 
                var gun = 1;

                foreach (double kur in Model)
                {
                    gun--;
                    <tr>
                        <td>@kur.ToString("#.##")</td>
                        <td>@DateTime.Today.AddDays(gun)</td>
                    </tr>
                    }
                }
        </tbody>
    </table>
